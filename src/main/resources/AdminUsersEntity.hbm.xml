<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.team180.tables.AdminUsersEntity" table="admin_users" schema="oneeighty">
        <id name="adminId">
            <column name="adminID" sql-type="int(11)"/>
        </id>
        <property name="firstName">
            <column name="first_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="lastName">
            <column name="last_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>